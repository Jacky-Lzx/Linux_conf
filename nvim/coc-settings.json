{
    "coc.preferences.extensionUpdateCheck": "daily",
    "snippets.userSnippetsDirectory": "~/.config/nvim/UltiSnips",
    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    "coc.preferences.semanticTokensHighlights": false,
    "coc.preferences.enableFloatHighlight": true,
    // "coc.preferences.snippetStatusText": "*",
    "diagnostic.checkCurrentLine": false, // show error on cursor line
    "diagnostic.signOffset": 1,
    "diagnostic.errorSign":"✘",
    "diagnostic.warningSign": "\uf071",
    "diagnostic.infoSign": "\uf05a",
    "diagnostic.hintSign": "✓",
    // diagnostic.errorSign":"\uf05a",
    //"diagnostic.warningSign": "\uf05a",
    //"diagnostic.infoSign": "\uf05a",
    //"diagnostic.hintSign": "\uf05a",
    "diagnostic.refreshOnInsertMode": false,
    "snippets.ultisnips.directories": [
        "$HOME/.config/nvim/Ultisnips/",
        "$HOME/vimfiles/plugged/vim-snippets/UltiSnips/"
    ],

    "cSpell.userWords": [
      "Zexi",
      "markdownlint"
    ],

    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "constructor": "\uf0ad",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        // "snippet": "\ue60b",
        "snippet": "\ue796",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },

    "translator.engines": [
        "bing",
        "youdao",
        "haici"
    ]
}
